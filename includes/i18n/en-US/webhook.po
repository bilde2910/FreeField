msgid "webhook.report_title"
msgstr "Field research reported!"

msgid "webhook.evil_title"
msgstr "Team Rocket spotted!"

msgid "webhook.poi"
msgstr "Pokéstop"

msgid "webhook.click_for_directions"
msgstr "Click here for directions"

msgid "webhook.objective"
msgstr "Objective"

msgid "webhook.reward"
msgstr "Reward"

#  %1 = Nickname of user who reported research
msgid "webhook.reported_by"
msgstr "Reported by {%1}"

#  %1 = Research objective (e.g. "Make 3 Great Throws")
#  %2 = Research reward (e.g. "1 Max Revive")
msgid "webhook.x_for_y"
msgstr "{%1} for {%2}"

#  %1 = Number of minutes Team Rocket stays active.
msgid "webhook.evil_disclaimer"
msgstr "Note: Team Rocket only stays on a Pokéstop for {%1} minutes after its spawn time, regardless of when it is reported here, and may thus disappear sooner than expected!"

#  This means "link" as in "link to a website".
#  Example context: "Reported by UserName @ link" where "link" is a hyperlink
#  to the local FreeField website.
msgid "webhook.link"
msgstr "link"
